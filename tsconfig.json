{
    "compilerOptions": {
      "moduleResolution": "node",
      "module": "commonjs",
      "target": "es2015",

      "declaration": true,

      "allowUnreachableCode": false,
      "allowUnusedLabels": false,
      "alwaysStrict": true,
      "noImplicitAny": true,
      "noUnusedLocals": true
    }
}
